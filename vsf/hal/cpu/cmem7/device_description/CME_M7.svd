            </field>
          </fields>
        </register>
      </registers>
    </peripheral>

    <!-- DDRC -->
    <peripheral>
      <name>DDRC</name>
      <version>1.0</version>
      <description>DDR_CFG</description>
      <groupName>DDR</groupName>
			<baseAddress>0x41400000</baseAddress>
      <access>read-write</access>
      <size>32</size>

      <addressBlock>
        <offset>0</offset>
        <size>0xB5</size>
