    <!-- AES -->
    <peripheral>
      <name>AES</name>
      <version>1.0</version>
      <description>AES</description>
      <groupName>AES</groupName>
			<baseAddress>0x41600000</baseAddress>
      <access>read-write</access>
      <size>32</size>

      <addressBlock>
        <offset>0</offset>
        <size>0x14</size>
